{
    "name": "amethyst/http-requester",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-http-requester", "http-requester"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "amethyst/common": "0.1.*",
        "amethyst/data-builder": "0.1.*",
        "amethyst/http-log": "0.1.*",
        "guzzlehttp/guzzle": "^6.3",
        "namshi/cuzzle": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "http-requester",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\HttpRequesterServiceProvider"
            ]
        }
    }
}
