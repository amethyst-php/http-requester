{
    "name": "railken/amethyst-http-requester",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-http-requester", "http-requester"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "railken/amethyst-data-builder": "0.1.*",
        "railken/amethyst-http-log": "0.1.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "http-requester",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\HttpRequesterServiceProvider"
            ]
        }
    }
}
